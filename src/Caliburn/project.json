{
  "version": "3.0.0-rc1-*",
  "description": "Caliburn",
  "authors": [
    "Rob Eisenberg",
    "Marco Amendola"
  ],
  "packOptions": {
    "tags": [
      "mvvm",
      "mvp",
      "wpf"
    ],
    "projectUrl": "https://github.com/CoreyKaylor/Caliburn"
  },
  "buildOptions": {
    "keyFile": "../Caliburn.snk"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "PresentationCore": "",
        "PresentationFramework": "",
        "System.ComponentModel.DataAnnotations": "",
        "System.Xaml": "",
        "System.Xml": "",
        "WindowsBase": ""
      }
    },
    "sl5": {
      "buildOptions": {
        "define": ["SILVERLIGHT", "NO_EXCEPTION_SERIALIZATION"]
      },
      "frameworkAssemblies": {
        "System": "",
        "mscorlib": "",
        "System.Core": "",
        "System.Windows": "",
        "System.Windows.Browser": "5.0.5.0",
        "System.Xml": "5.0.5.0"
      },
      "dependencies": {
        "System.ComponentModel.DataAnnotations": { "target": "project" },
        "System.Windows.Controls": {"target": "project" }
      }
    } 
  }
}